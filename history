   51  wget https://raw.githubusercontent.com/Angristan/openvpn-install/master/openvpn-install.sh -O centos7-vpn.sh
   53  chmod +x centos7-vpn.sh
   55  sh centos7-vpn.sh
   56  cat /root/iphone.ovpn
   58  systemctl start openvpn-server@server.service



